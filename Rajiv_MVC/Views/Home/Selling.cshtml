
@{
    ViewBag.Title = "Selling";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>the gtalistings</title>
    <!-- Bootstrap -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="~/css/font.css" rel="stylesheet">
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/animate.min.css" rel="stylesheet">
    <link href="~/css/media-queries.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/nivo-slider.css" type="text/css" media="screen" />

    @*<script src="~/js/npm.js"></script>*@

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script>
        $(function () {
            debugger;
            var activeurl = window.location.pathname;
            var myString = activeurl.substr(activeurl.indexOf("/") + 6)
            if (myString != "") {
                var latest = $('a[class="' + myString + '"]');
                var prev = $('.navbar-nav li').removeClass('active');
                latest.parent('li').addClass('active');
            }
            else {
                $('a[class="Index"]').parent('li').addClass('active');
            }

        });
    </script>
    <script src="~/js/bootstrap.min.js"></script>

    <script type="text/javascript" src="~/js/jquery.nivo.slider.js"></script>

    <script type="text/javascript">
        $(window).load(function () {
            $('#slider').nivoSlider();
        });
    </script>
</head>
<body>
    <div class="top_section_bg">
        <div class="container">
            <div class="top_section">
                <div class="col-md-1 col-sm-1"></div>
                <div class="col-md-10 col-sm-10">
                    <div class="menu_sction">
                        <nav role="navigation" class="navbar navbar-default">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <button type="button" data-target="#navbarCollapse" data-toggle="collapse" class="navbar-toggle">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </div>
                            <!-- Collection of nav links and other content for toggling -->
                            <div id="navbarCollapse" class="collapse navbar-collapse">
                                <ul class="nav navbar-nav">
                                    <li class="active"><a class="Index" href="../Home/Index">Home</a></li>
                                    <li><a class="Selling" href="../Home/Selling">Selling on Assignment</a></li>
                                    <li><a class="Buying" href="../Home/Buying">Buying on Assignment</a></li>
                                    <li><a class="FAQ" href="../Home/Faq">FAQ</a></li>
                                    <li><a class="contact" href="../Home/contact">Contact us</a></li>
                                </ul>

                            </div>
                        </nav>
                    </div>
                </div>
                <div class="col-md-1 col-sm-1"></div>
            </div>
        </div>
    </div>

    <div class="banner_section">
        <div class="inner_banner">
            <img src="~/images/banner1.jpg" data-thumb="~/images/banner1.jpg" alt="" />
        </div>
    </div>

    <div class="miidle_section_bg">
        <div class="container">
            <div class="services_bg">
                <div class="col-md-3 col-sm-3">
                </div>
                <div class="col-md-6 col-sm-6 fadeIn">
                    <div class="services_sctn">
                        <h2>Selling on Assignment</h2>
                        <form role="form">
                            <p>We Have Buyers for You. Waiting.</p>
                            <hr class="colorgraph">

                            <div class="row">
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="username" id="first_name" class="form-control input-lg" placeholder="First Name:" tabindex="1">
                                        <label id="message1" class="error"></label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="lastname" id="last_name" class="form-control input-lg" placeholder="Last Name:" tabindex="2">
                                        <label id="message2" class="error"></label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <input type="text" name="phn" id="phone" class="form-control input-lg" placeholder="Phone No:" tabindex="1">
                                <label id="message3" class="error"></label>
                            </div>
                            <div class="form-group">
                                <input type="email" name="email" id="e-mail_id" class="form-control input-lg" placeholder="E-Mail Address:" tabindex="2">
                                <label id="message4" class="error"></label>
                            </div>
                            <div class="row">
                                <div class="midle_frm">
                                    <p>If Selling, Property Address</p>
                                </div>
                            </div>



                            <div class="row">
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="street" id="first_name" class="form-control input-lg" placeholder="Street#:" tabindex="1">

                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        <input type="text" name="unit" id="last_name" class="form-control input-lg" placeholder="Unit(If Condo):" tabindex="2">

                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <input type="text" name="streetName" id="display_name" class="form-control input-lg" placeholder="Street Name:" tabindex="3">

                            </div>
                            <div class="form-group">
                                <input type="text" name="city" id="email" class="form-control input-lg" placeholder="City:" tabindex="4">

                            </div>
                            <div class="row">
                                <div class="col-xs-3 col-md-3"></div>
                                <div class="col-xs-6 col-md-6">
                                    <button id="btnSelling" type="button" class="btn btn-success btn-block btn-lg frm_bttn">Submit</button>
                                    @*<a href="#" class="btn btn-success btn-block btn-lg frm_bttn">Submit</a>*@
                                </div>
                                <div class="col-xs-3 col-md-3"></div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-md-3 col-sm-3">
                </div>
            </div>



        </div>
    </div>

    <div class="futer_sctn_bg">
        <div class="container">
            <div class="col-md-12 col-sm-12">
                <div class="footer">
                    <p><a href="../Home/Privacy">Privacy Policy</a>&nbsp; &nbsp; &nbsp; &nbsp; Designed & Developed by <a href="http://only4agents.com/" target="_blank">Only4Agents</a></p>
                </div>
            </div>
        </div>
    </div>


</body>
</html>

<script src="http://code.jquery.com/jquery-latest.min.js"
        type="text/javascript"></script>
<script>

    $(document).ready(function () {


        $('#btnSelling').click(function () {
            debugger;
            var street = $("input[name=street]").val();
            var unit = $("input[name=unit]").val();
            var streetName = $("input[name=streetName]").val();
            var city = $("input[name=city]").val();
            if ($("input[name=username]").val() == "") {
                $('#message1').html('This field is required');
                $('#message2').html('');
                $('#message3').html('');
                $('#message4').html('');
                $('#message5').html('');
            }
            else if ($("input[name=lastname]").val() == "") {
                $('#message2').html('This field is required');
                $('#message1').html('');
                $('#message3').html('');
                $('#message4').html('');
                $('#message5').html('');
            }
            else if ($("input[name=phn]").val() == "") {
                $('#message3').html('This field is required');
                $('#message1').html('');
                $('#message2').html('');
                $('#message4').html('');
                $('#message5').html('');
            }
            else if ($("input[name=email]").val() == "") {
                $('#message4').html('This field is required');
                $('#message1').html('');
                $('#message2').html('');
                $('#message3').html('');
                $('#message5').html('');
            }

            else if ($("textarea[name=message]").val() == "") {
                $('#message5').html('This field is required');
                $('#message1').html('');
                $('#message3').html('');
                $('#message2').html('');
                $('#message4').html('');
            }

            if ($("input[name=username]").val() != "" && $("input[name=lastname]").val() != "" && $("input[name=email]").val() != "" && $("input[name=phn]").val() != "" && $("textarea[name=message]").val() != "") {

                $.ajax({

                    url: "/Home/Selling",
                    data: { 'username': $("input[name=username]").val(), 'lastname': $("input[name=lastname]").val(), 'Email': $("input[name=email]").val(), 'phn': $("input[name=phn]").val(), 'message': $("textarea[name=message]").val(), 'street': street, 'unit': unit, 'streetName': streetName, 'city': city },
                    type: "POST",
                    dataType: 'json',
                    success: function (data) {
                        debugger;
                        alert('Mail has been sent to admin. You will be contacted shortly.');
                        $('#message1').html('');
                        $('#message3').html('');
                        $('#message2').html('');
                        $('#message4').html('');
                        $('#message5').html('');
                        $("input[name=username]").val('');
                        $("input[name=lastname]").val('');
                        $("input[name=email]").val('');
                        $("input[name=phn]").val('');
                        $("input[name=street]").val('');
                        $("input[name=unit]").val('');
                        $("input[name=city]").val('');
                        $("input[name=streetName]").val('');
                        return false;
                    },
                    error: function () {

                        alert("Dynamic content load failed.");
                    }
                });
            }
        });
    });
</script>
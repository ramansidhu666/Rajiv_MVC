<script>
   
    $(document).ready(function () {

        $('#btnBuying').click(function () {
            debugger;
            var street = $("#SearchBar_ddlTyps option:selected").text();
            var unit = $("#SearchBar_ddlBeds option:selected").text();
            var streetName = $("#SearchBar_ddlBaths option:selected").text();
            var city = $("input[name=city1]").val();
            if ($("input[name=username1]").val() == "") {
                $('#message5').html('This field is required');
                $('#message6').html('');
                $('#message7').html('');
                $('#message8').html('');
            }
            else if ($("input[name=lastname1]").val() == "") {
                $('#message6').html('This field is required');
                $('#message5').html('');
                $('#message7').html('');
                $('#message8').html('');
            }
            else if ($("input[name=phn1]").val() == "") {
                $('#message7').html('This field is required');
                $('#message6').html('');
                $('#message5').html('');
                $('#message8').html('');
            }
            else if ($("input[name=email1]").val() == "") {
                $('#message8').html('This field is required');
                $('#message6').html('');
                $('#message5').html('');
                $('#message7').html('');
            }

           
            if ($("input[name=username1]").val() != "" && $("input[name=lastname1]").val() != "" && $("input[name=email1]").val() != "" && $("input[name=phn1]").val() != "" ) {

                $.ajax({

                    url: "/Home/Buying",
                    data: { 'username': $("input[name=username1]").val(), 'lastname': $("input[name=lastname1]").val(), 'Email': $("input[name=email1]").val(), 'phn': $("input[name=phn1]").val(), 'street': street, 'unit': unit, 'streetName': streetName, 'city': city },
                    type: "POST",
                    dataType: 'json',
                    success: function (data) {
                        debugger;
                        alert('Mail has been sent to admin. You will be contacted shortly.');
                        $('#message5').html('');
                        $('#message6').html('');
                        $('#message7').html('');
                        $('#message8').html('');
                       
                        $("input[name=username1]").val('');
                        $("input[name=lastname1]").val('');
                        $("input[name=email1]").val('');
                        $("input[name=phn1]").val('');
                      
                    
                        $("input[name=city1]").val('');
                        return false;
                    },
                    error: function () {

                        alert("Dynamic content load failed.");
                    }
                });
            }
        });
        $('#btnSelling').click(function () {
            debugger;
            var street = $("input[name=street]").val();
            var unit = $("input[name=unit]").val();
            var streetName = $("input[name=streetName]").val();
            var city = $("input[name=city]").val();
            if ($("input[name=username]").val() == "") {
                $('#message1').html('This field is required');
                $('#message2').html('');
                $('#message3').html('');
                $('#message4').html('');
                $('#message5').html('');
            }
            else if ($("input[name=lastname]").val() == "") {
                $('#message2').html('This field is required');
                $('#message1').html('');
                $('#message3').html('');
                $('#message4').html('');
                $('#message5').html('');
            }
            else if ($("input[name=phn]").val() == "") {
                $('#message3').html('This field is required');
                $('#message1').html('');
                $('#message2').html('');
                $('#message4').html('');
                $('#message5').html('');
            }
            else if ($("input[name=email]").val() == "") {
                $('#message4').html('This field is required');
                $('#message1').html('');
                $('#message2').html('');
                $('#message3').html('');
                $('#message5').html('');
            }

            else if ($("textarea[name=message]").val() == "") {
                $('#message5').html('This field is required');
                $('#message1').html('');
                $('#message3').html('');
                $('#message2').html('');
                $('#message4').html('');
            }

            if ($("input[name=username]").val() != "" && $("input[name=lastname]").val() != "" && $("input[name=email]").val() != "" && $("input[name=phn]").val() != "" && $("textarea[name=message]").val() != "") {

                $.ajax({

                    url: "/Home/Selling",
                    data: { 'username': $("input[name=username]").val(), 'lastname': $("input[name=lastname]").val(), 'Email': $("input[name=email]").val(), 'phn': $("input[name=phn]").val(), 'message': $("textarea[name=message]").val(), 'street': street,'unit':unit,'streetName':streetName,'city':city },
                    type: "POST",
                    dataType: 'json',
                    success: function (data) {
                        debugger;
                        alert('Mail has been sent to admin. You will be contacted shortly.');
                        $('#message1').html('');
                        $('#message3').html('');
                        $('#message2').html('');
                        $('#message4').html('');
                        $('#message5').html('');
                        $("input[name=username]").val('');
                        $("input[name=lastname]").val('');
                        $("input[name=email]").val('');
                        $("input[name=phn]").val('');
                        $("input[name=street]").val('');
                        $("input[name=unit]").val('');
                        $("input[name=city]").val('');
                        $("input[name=streetName]").val('');
                        return false;
                    },
                    error: function () {

                        alert("Dynamic content load failed.");
                    }
                });
            }
        });
    });
</script>

